    "connect-flash": "^0.1.1",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-device": "^0.4.2",
    "jest": "26.6.0",
    "mongodb-memory-server": "^7.4.0",
    "mongoose": "^6.0.2",
    "node-cron": "^3.0.2",
    "nodemon": "^2.0.12",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "react-redux": "^7.2.4",
    "redux": "^4.1.1",
    "ssl-express-www": "^3.0.8",
    "supertest": "^6.1.6",
    "web-push": "^3.4.5"
